{
  "name": "survey-hub",
  "version": "0.0.0",
  "description": "",
  "main": "server/index.js",
  "engines": {
    "node": "11.2.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "build:client": "npm run build --prefix client",
    "build:server": "babel server --out-dir server/dist --ignore server/dist",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "heroku-postbuild": "npm run build:server && npm install --prefix client && npm run build:client",
    "start": "node server/dist/index",
    "start:client": "npm run start --prefix client",
    "start:server": "nodemon --ignore client/* --exec babel-node --inspect server/index",
    "share-localhost": "lt --port 5000 --subdomain 405fd553"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.17.3",
    "mongoose": "^5.3.13",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "path-parser": "^4.2.0",
    "sendgrid": "^5.2.3",
    "stripe": "^6.18.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "localtunnel": "^1.9.1",
    "nodemon": "^1.18.6"
  }
}
